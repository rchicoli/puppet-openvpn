<% @iroute.each do |iroute| -%>
iroute <%= iroute %>
<% end -%>
<% if ifconfig = @ifconfig -%>
ifconfig-push <%= ifconfig %>
<% end -%>
<% @dhcp_options.each do |option| -%>
push dhcp-option <%= option %>
<% end -%>
<% if @redirect_gateway -%>
push redirect-gateway def1
<% end -%>
<% @route.each do |route| -%>
push "route <%= route %>"
<% end -%>

