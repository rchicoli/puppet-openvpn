client
ca keys/<%= @name %>/ca.crt
cert keys/<%= @name %>/<%= @name %>.crt
key keys/<%= @name %>/<%= @name %>.key
dev <%= @dev %>
proto <%= @proto %>
remote <%= @remote_host %> <%= @port %>
<% if @compression != '' -%>
<%= @compression %>
<% end -%>
resolv-retry <%= @resolv_retry %>
auth-retry <%= @auth_retry %>
<% if @nobind -%>
nobind
<% end -%>
<% if @persist_key -%>
persist-key
<% end -%>
<% if @persist_tun -%>
persist-tun
<% end -%>
<% if @cipher  -%>
cipher <%= @cipher %>
<% end %>
<% if @mute_replay_warnings -%>
mute-replay-warnings
<% end -%>
ns-cert-type server
verb <%= @verb %>
mute <%= @mute %>
<% if @pam || @authuserpass -%>
auth-user-pass
<% end -%>
<% @setenv.each do |key, value| -%>
setenv <%= key %> <%= value %>
<% end -%>
<% @setenv_safe.each do |key, value| -%>
setenv_safe <%= key %> <%= value %>
<% end -%>
<% if @up != '' or @down != ''-%>
script-security 2
<% end -%>
<% if @up != '' -%>
up "<%= @up %>"
<% end -%>
<% if @down != '' -%>
down "<%= @down %>"
<% end -%>
